{
  "navigationFallback": {
    "rewrite": "/public/index.html",
    "exclude": ["/public/assets/*", "/public/css/*", "/public/js/*"]
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/public/index.html"
    },
    {
      "route": "/gobiernos/*",
      "rewrite": "/public/gobiernos/index.html"
    },
    {
      "route": "/empresas/*",
      "rewrite": "/public/empresas/index.html"
    },
    {
      "route": "/legal/*",
      "rewrite": "/public/legal/*.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/public/index.html"
    }
  }
}